{
  "name": "gedcom",
  "version": "3.0.2",
  "type": "module",
  "description": "a simple and readable gedcom parser",
  "source": "index.ts",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "esmodule": "dist/index.modern.js",
  "main:umd": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "bin": {
    "gedcom": "dist/cli.js"
  },
  "files": ["dist"],
  "scripts": {
    "test": "vitest run",
    "doc": "typedoc --plugin typedoc-plugin-markdown --readme none index.ts",
    "lint": "biome check",
    "prepublishOnly": "npm run build",
    "build": "microbundle && microbundle -i cli.ts -o dist/cli.js -f cjs && npm run doc"
  },
  "keywords": ["gedcom", "format", "geneaology"],
  "author": "Tom MacWright",
  "license": "ISC",
  "dependencies": {
    "@types/graphlib": "^2.1.12",
    "@types/graphlib-dot": "^0.6.4",
    "@types/unist": "^3.0.3",
    "get-stdin": "^9.0.0",
    "graphlib": "^2.1.8",
    "graphlib-dot": "^0.6.4",
    "meow": "^13.2.0",
    "tslib": "^2.8.1",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit-parents": "^6.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "microbundle": "^0.15.1",
    "typedoc": "^0.27.6",
    "typedoc-plugin-markdown": "^4.4.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tmcw/parse-gedcom.git"
  }
}
